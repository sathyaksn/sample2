var obj1 = {"name":"sathya","age":"22"};
var obj2 = {"age":"22","name":"sathya"};

var flag=true;

if(Object.keys(obj1).length==Object.keys(obj2).length){
    for(let key in obj1) { 
        if(obj1[key] == obj2[key]) {
        continue;
        }
        else {
            flag=false;
            break;
        }
    }
}
else {
    flag=false;
}
console.log(" object is equal"+flag);